    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${env:USERPROFILE}/.dotnet/tools/dotnet-lambda-test-tool-6.0.exe",
            "args": [],
            "cwd": "${workspaceFolder}/StepStarterLambda/src/StepStarterLambda",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "Region": "eu-north-1",
                "StateMachineArn":"arn:aws:states:eu-north-1:761241929638:stateMachine:CostHubAPI",
                "Synchronous": "true"
              }
        }
    ]
